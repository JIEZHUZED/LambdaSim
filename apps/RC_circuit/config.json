{
  "aws":{
    "region": "us-west-1"
  },
  "lambda":{
    "function_name": "rc_circuit",
    "timeout": 60,
    "description": "A simple function that simulates an RC circuit",
    "memory_size": 256
  },
  "model":{
    "fmu_name": "SimpleElectrical_Examples_RC.fmu",
    "solver": "CVode",
    "tolerance": 0.00001,
    "max_sim_time": 10000.0,
    "min_sim_time": 0.0,
    "n_points": {"default": 500, "min":300, "max":1000},
    "params": {
      "whitelist": null,
      "blacklist": null
    },
    "variables": {
      "whitelist": null,
      "blacklist": null
    }
  }
}