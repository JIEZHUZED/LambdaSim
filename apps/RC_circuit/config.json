{
  "aws":{
    "region": "us-west-1"
  },
  "lambda":{
    "function_name": "rc_circuit",
    "timeout": 60,
    "description": "A simple function that simulates an RC circuit",
    "memory_size": 256
  },
  "model":{
    "fmu_name": "SimpleElectrical_Examples_RC.fmu",
    "options": {},
    "simulation_time": {
      "min": 0.0,
      "max": 100.0
    },
    "n_points": {
      "default": 200,
      "min": 100,
      "max": 500
    },
    "params": {
      "whitelist": null,
      "blacklist": null
    },
    "variables": {
      "whitelist": null,
      "blacklist": null
    }
  }
}