{
  "aws":{
    "region": "us-west-1"
  },
  "lambda":{
    "function_name": "rc_circuit",
    "timeout": 60,
    "description": "A simple function that simulates an RC circuit",
    "memory_size": 256,
    "access_control_allow_origin": "*"
  },
  "s3": {
    "folder": "rc_circuit"
  },
  "model":{
    "fmu_name": "SimpleElectrical_Examples_RC.fmu",
    "input_files": [],
    "options": {},
    "simulation_time": {
      "min": 0.0,
      "max": 100.0
    },
    "n_points": {
      "default": 200,
      "min": 100,
      "max": 500
    }
  }
}